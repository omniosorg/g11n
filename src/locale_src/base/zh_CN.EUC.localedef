# Date & Time Format
# 1980 YEAR 5 MONTH 6 DATE  Monday 8 HOUR 30 MINUTE 0 SECOND
# d_t_fmt "%Y<gb3674>%m<gb5234>%d<gb4053>\
#	<space>%A<space>%H<gb4217>%M<gb2354>%S<gb3575>"
d_t_fmt "<percent-sign><Y><gb3674><percent-sign><m><gb5234><percent-sign><d><gb4053><space><percent-sign><A><space><percent-sign><H><gb4217><percent-sign><M><gb2354><percent-sign><S><gb3575>"

# Date Format
# 1980 YEAR 5 MONTH 6 DATE
# d_fmt 	"%Y<gb3674>%m<gb5234>%d<gb4053>"
d_fmt 	"<percent-sign><Y><gb3674><percent-sign><m><gb5234><percent-sign><d><gb4053>"

# Time Format
# 8 HOUR 30 MINUTE 0 SECOND
# t_fmt   "%H<gb4217>%M<gb2354>%S<gb3575>"
t_fmt   "<percent-sign><H><gb4217><percent-sign><M><gb2354><percent-sign><S><gb3575>"

# AM and PM in Chinese Ideograms
am_pm 	"<gb4147><gb4671>";"<gb4734><gb4671>"

# Time format using am/pm
# t_fmt_ampm	"%p%I<gb4217>%M<gb2354>%S<gb3575>"
