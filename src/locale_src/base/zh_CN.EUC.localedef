# date_fmt "%Y<gb3674>%m<gb5234>%d<gb4053><space>%A<space>\
#	   %H<gb4217>%M<gb2354>%S<gb3575>%Z"
date_fmt "<percent-sign><c><space><percent-sign><Z>"

